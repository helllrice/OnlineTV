-var pageTitle = 'Online TV'

doctype html 
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(name="viewport" content="width=device-width, initial-scale=1.0")
        title= pageTitle
        link(rel="stylesheet", type="text/css", href="src/sass/style.scss")
    body
        include modules/header
        include modules/banner
        include modules/movies
        include modules/genres
        include modules/actors
        include modules/next-film
        include modules/register
        include modules/footer
        script(src="js/script.js") 